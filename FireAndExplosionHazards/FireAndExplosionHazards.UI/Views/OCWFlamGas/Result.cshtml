@model FireAndExplosionHazards.BLL.Concrete.OCW.FlamGas

<h2>Результаты:</h2>

<div class="form-horizontal">
    <div class="form-group">
        @Html.Label("Объем газа, который вышел из трубопровода после перекрытия", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            V2т = @Html.Label(@Model.V2т().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Объем газа, который вышел из трубопровода до перекрытия", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            V1т = @Html.Label(@Model.V1т().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Объем газа, который вышел из трубопроводов", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            Vт = @Html.Label(@Model.Vт().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Масса газа", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            m = @Html.Label(@Model.m().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Стехиометрический коэффициент кислорода в реакции горения ", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            β = @Html.Label(@Model.β().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Cтехиометрическая концентрация паров ЛВЖ и ГЖ", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            Cct = @Html.Label(@Model.Cct().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Плотность пара", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            Pгп = @Html.Label(@Model.Pгп().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Избыточного давления взрыва для горючих газов", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            ΔР = @Html.Label(@Model.ΔР().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class=" form-group">
        <div class="col-md-12">
            @ViewBag.message
        </div>
    </div>
</div>