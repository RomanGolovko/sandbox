@model FireAndExplosionHazards.BLL.Concrete.OC.CombDust

<h2>Результаты:</h2>

<div class="form-horizontal">
    <div class="form-group">
        @Html.Label("Масса пыли в доступные местах", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            m2 = @Html.Label(@Model.m2().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Масса пыли в труднодоступные местах", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            m1 = @Html.Label(@Model.m1().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Масса пыли, которая отложилась в помещении к моменту аварии", htmlAttributes: new { @class = "control-label col-md-4" })
        <div class="col-md-8">
            mп = @Html.Label(@Model.mп().ToString(), htmlAttributes: new { @class = "control-label" })
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Расчетная масса пыли, которая поступила в помещение в результате аварийной ситуации из аппаратов и технологического оборудования",
            htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            mав = @Html.Label(@Model.mав().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Расчетная масса части отложившейся в помещении пыли, которая перешла в состояние аэрозоля", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            mвз = @Html.Label(@Model.mвз().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Масса пыли", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            m = @Html.Label(@Model.m().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Избыточного давления взрыва для горючей пыли",
        htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            ΔР = @Html.Label(@Model.ΔР().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class=" form-group">
        <div class="col-md-12">
            @ViewBag.message
        </div>
    </div>
</div>