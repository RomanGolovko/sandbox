@model FireAndExplosionHazards.BLL.Concrete.OC.FlamLiq

<h2>Результаты:</h2>

<div class="form-horizontal">
    <div class="form-group">
        @Html.Label("Давление насыщенного пара", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            <strong>Pн = </strong>@Html.Label(Model.Pн().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Интенсивность испарения", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            <strong>W = </strong>@Html.Label(Model.W().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Масса жидкости", htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            <strong>m = </strong>@Html.Label(Model.m().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class="form-group">
        @Html.Label("Избыточного давления взрыва для паров легковоспламеняющихся и горючих жидкостей",
                    htmlAttributes: new { @class = "control-label col-md-4" })
        <strong>
            <strong>ΔР = </strong>@Html.Label(Model.ΔР().ToString(), htmlAttributes: new { @class = "control-label" })
        </strong>
    </div>

    <div class=" form-group">
        <div class="col-md-12">
            @ViewBag.message
        </div>
    </div>
</div>