@model FireAndExplosionHazards.BLL.Concrete.OCW.FlamLiq

<h2>Результаты:</h2>

<div class="form-horizontal">
    <div class="form-group">
        @Html.Label("Давление насыщенного пара", htmlAttributes: new { @class = "control-label col-md-4" })
        <div class="col-md-8">
            Pн = @Html.Label(Model.Pн().ToString())
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Интенсивность испарения", htmlAttributes: new { @class = "control-label col-md-4" })
        <div class="col-md-8">
            W = @Html.Label(Model.W().ToString())
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Масса жидкости", htmlAttributes: new { @class = "control-label col-md-4" })
        <div class="col-md-8">
            m = @Html.Label(Model.m().ToString())
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Стехиометрический коэффициент кислорода в реакции горения", htmlAttributes: new { @class = "control-label col-md-4" })
        <div class="col-md-8">
            β = @Html.Label(Model.β().ToString())
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Cтехиометрическая концентрация паров легковоспламеняющихся и горючих жидкостей", htmlAttributes: new { @class = "control-label col-md-4" })
        <div class="col-md-8">
            Cct = @Html.Label(Model.Cct().ToString())
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Плотность пара", htmlAttributes: new { @class = "control-label col-md-4" })
        <div class="col-md-8">
            Pгп = @Html.Label(Model.Pгп().ToString())
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Избыточного давления взрыва для паров легковоспламеняющихся и горючих жидкостей",
                    htmlAttributes: new { @class = "control-label col-md-4" })
        <div class="col-md-8">
            ΔР = @Html.Label(Model.ΔР().ToString())
        </div>
    </div>

    <div class=" form-group">
        <div class="col-md-12">
            @ViewBag.message
        </div>
    </div>
</div>